generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  schemas  = ["session"]
}

model Sessions {
  id        String   @id @default(cuid())
  sessionId String
  credId    String
  data      String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@unique([sessionId, credId], map: "unique_credid_per_session")
  @@index([sessionId])
  @@schema("session")
}
